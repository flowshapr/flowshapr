# Google AI API Key (required for AI model integration)
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# OpenAI API Key (optional - for OpenAI model support)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (optional - for Claude model support)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Next.js Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Development Environment
NODE_ENV=development

# Backend API URL
BACKEND_URL=http://localhost:3001

# Better Auth configuration (frontend)
NEXT_PUBLIC_BETTER_AUTH_URL=http://localhost:3001

# Social Auth Provider Keys (for display purposes only - actual auth handled by backend)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id
NEXT_PUBLIC_GITHUB_CLIENT_ID=your-github-client-id
NEXT_PUBLIC_MICROSOFT_CLIENT_ID=your-microsoft-client-id
NEXT_PUBLIC_APPLE_CLIENT_ID=your-apple-client-id